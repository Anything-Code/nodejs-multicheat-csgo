{
  "name": "csgo-multicheat",
  "version": "0.2.0",
  "description": "A multicheat for csgo with a settings-menu that runs on localhost.",
  "repository": {},
  "private": true,
  "main": "main.js",
  "scripts": {
    "dist": "npm run distribute",
    "distribute": "cross-env rm -rf dist/nodejs-multicheat-csgo/build && cp -r build dist/nodejs-multicheat-csgo",
    "build": "cross-env webpack --progress --config=webpack.conf.js && rm -rf build/static && cp -r src/static build",
    "start": "cross-env node-v11.15.0-win-x86/node src/main",
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm mix-manifest.json",
    "prod": "npm run production"
  },
  "author": "Niklas LÃ¼bcke",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "asynckeystate": "0.0.2",
    "colors": "^1.3.2",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "iohook": "^0.6.5",
    "keycode": "^2.2.0",
    "laravel-mix": "^3.0.0",
    "memoryjs": "^3.2.3",
    "node-sass": "^4.13.1",
    "raleway-webfont": "^3.0.1",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "vue": "^2.6.11",
    "vue-router": "^2.7.0",
    "vue-socket.io": "^3.0.4",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^1.5.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
